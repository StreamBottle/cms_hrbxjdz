<%- include header %>
	<header id="gtco-header" class="gtco-cover gtco-cover-xs" role="banner" style="background-image:url(/<%=CONST.SYSTEM.THEME%>/images/bg01.jpg);">
		<div class="overlay"></div>
		<div class="gtco-container">
			<div class="row">
				<div class="col-md-8 col-md-offset-2 text-center">
					<div class="display-t">
						<div class="display-tc">
							<h1 class="animate-box" data-animate-effect="fadeInUp">关于新金</h1>
							<h2 class="animate-box" data-animate-effect="fadeInUp">About Us</h2>
						</div>
					</div>
				</div>
			</div>
		</div>
	</header>
	<div class="gtco-section border-bottom animate-box fadeInUp animated-fast" id="gtco-history">
		<div class="gtco-container">
				
			<div class="row">
				<div class="col-md-8 col-md-offset-2 text-center gtco-heading">
					<h2><%-smac[0].title%></h2>
					<p><%-smac[0].description%></p>
				</div>
			</div>

			<div class="row row-pb-md">
				<div class="col-md-12">
					<%-smac[0].acontent%>
				</div>
			</div>

		</div>
	</div>
<%- include footer %>